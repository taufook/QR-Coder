{
  "manifest_version": 2,

  "name": "QR Coder",
  "description": "This extension creates QRCodes simply.",
  "version": "0.1.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Create QRCode"
  },
  "permissions": [
    "activeTab"
  ]
}